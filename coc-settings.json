{
    "diagnostic.enable":false,
    "languageserver": {
        "cquery": {
            "command": "cquery",
            "args": ["--log-file=/tmp/cq.log"],
            "filetypes": ["c", "cpp"],
            "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vimproject/"],
            "initializationOptions": {
                "cacheDirectory": "/tmp/cquery"
            }
        }
    }
}
